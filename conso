use Conso\{
    Conso,Input,Output
};

require 'vendor/autoload.php';

$conso = new Conso(new Input, new Output);

$conso->setSignature("_________
\_   ___ \  ____   ____   __________
/    \  \/ /  _ \ /    \ /  ___/  _ \
\     \___(  <_> )   |  \\___ (  <_> )
\______  /\____/|___|  /____  >____/
      \/            \/     \/
");

require_once 'commands.php';

$conso->run(0);